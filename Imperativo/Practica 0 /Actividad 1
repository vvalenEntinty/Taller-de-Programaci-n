c) program NumAleatorio;
var ale,i: integer;
begin
randomize;
for i := 1 to 20 do begin
ale := random (100);
writeln ('El número aleatorio generado es: ', ale);
end;
readln;
end.

d)program NumAleatorio;
var ale,i: integer;
N,A,B:integer;
begin
randomize;

readln(A);
readln(B);
readln(N);
for i := A to B do begin
ale := random (N);
writeln ('El número aleatorio generado es: ', ale);
end;
readln;
end.

e)program NumAleatorio;
var ale: integer;
A,B,F:integer;
begin
randomize;
readln(A);
readln(B);
readln(F);
ale := A + random (B-A+1);
while (ale <> F) do begin
writeln ('El número aleatorio generado es: ', ale);
ale := A +random (B+A-1);
end;
readln;
end.
